	<global>
		<helpers>
            <process>
                <class>Ccc_Process_Helper</class>
            </process>
        </helpers>
        <blocks>
            <process>
                <class>Ccc_Process_Block</class>
            </process>
        </blocks>
        <models>
            <process>
                <class>Ccc_Process_Model</class>
                <resourceModel>process_resource</resourceModel>
            </process>
            <process_resource>
                <class>Ccc_Process_Model_Resource</class>
                <entities>
					<process>
						<table>process</table>
					</process>
					<entry>
						<table>process_entry</table>
					</entry>
					<group>
						<table>process_group</table>
					</group>
					<column>
						<table>process_column</table>
					</column>
					<pro>
						<table>pro</table>
					</pro>
					<product>
						<table>product</table>
					</product>
					<category>
						<table>category</table>
					</category>
				</entities>
            </process_resource>
        </models>	
        <resources>
        	<!-- <process_setup>
        		<setup>
        			<module>Ccc_Process</module>
                    <class>Ccc_Process_Model_Resource_Setup</class>
